{
  "name": "@nuxtjs/cloudinary",
  "version": "1.0.3",
  "description": "Cloudinary module for Nuxt",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-cloudinary",
    "cloudinary",
    "images",
    "videos",
    "optimization",
    "plugins"
  ],
  "repository": "nuxt-community/cloudinary-module",
  "license": "MIT",
  "contributors": [
    {
      "name": "Maya Shavin <dpnminh@gmail.com>"
    }
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build  && mkdist --src src/runtime --dist dist/runtime",
    "dev": "nuxt-ts example",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "release": "yarn build && yarn test && standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && jest"
  },
  "husky": {
    "gitHooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "rollup-plugin-esbuild": "2.6.1"
  },
  "dependencies": {
    "@cld-apis/utils": "^0.2.0",
    "chalk": "^4.1.2",
    "cloudinary": "^1.37.0",
    "cloudinary-core": "^2.13.0",
    "cloudinary-vue": "^1.2.4",
    "consola": "^2.15.3",
    "defu": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@cld-apis/types": "^0.1.6",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@nuxt/test-utils": "latest",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@siroc/jest-preset": "latest",
    "@types/jest": "latest",
    "babel-eslint": "latest",
    "babel-jest": "latest",
    "eslint": "latest",
    "husky": "^6.0.0",
    "jest": "latest",
    "mkdist": "^0.4.1",
    "nuxt": "latest",
    "playwright": "^1.34.3",
    "siroc": "latest",
    "standard-version": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "14.21.3"
  }
}
